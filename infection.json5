{
  "$schema": "https://raw.githubusercontent.com/infection/infection/0.27.9/resources/schema.json",
  "source": {
    "directories": [
      "src"
    ]
  },
  "logs": {
    "html": "infection.html",
    "summaryJson": "infection.json",
    "github": true,
    "stryker": {
      "badge": "/^release-.*$/"
    }
  },
  "mutators": {
    "@default": true,
    "ProtectedVisibility": {
      "ignore": [
        "Zerotoprod\\ServiceModel\\Factory::definition"
      ]
    },
    "PublicVisibility": {
      "ignore": [
        "Zerotoprod\\ServiceModel\\Factory::definition"
      ]
    }
  },
  "testFramework": "pest"
}